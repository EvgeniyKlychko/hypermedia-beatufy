{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends":["prettier"],
    "plugins": [
        "babel",
        "import",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error"]
    }
}